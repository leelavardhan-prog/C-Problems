#include <stdio.h>

int main() {
    int N;
    int initialBalance;
    int transaction;
    int i = 0;
    int lowBalanceDays = 0;
    scanf("%d", &N);
    scanf("%d", &initialBalance);
    while(i < N) {
        scanf("%d", &transaction);
        initialBalance = initialBalance + transaction;
        if(initialBalance < 2000) {
            lowBalanceDays++;
        }
        i++;
    }
    printf("Final Balance: %d\n", initialBalance);
    printf("Low Balance Days: %d\n", lowBalanceDays);
    return 0;
}
